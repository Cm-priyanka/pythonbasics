if __name__ == '__main__':
    n = int(input())  # Read integer from STDIN

    # Loop through non-negative integers i < n and print i^2
    for i in range(n):
        print(i ** 2)
//second program

if __name__ == '__main__':
    n = int(input()) # Read integer from STDIN

    # Loop through integers 1 to n (inclusive)
    for i in range(1, n + 1):
        # Print each integer i without newline
        print(i, end='')
// hello world
